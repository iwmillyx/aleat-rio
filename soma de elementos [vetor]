#include<stdio.h>

int main(){
    
    int tamanho;
    
    printf("Digite o tamanho do vetor: ");
    scanf("%d", &tamanho);
    
    int vetor[tamanho];
    
    printf("Digite os elementos do vetor:\n");
    for(int i=0; i < tamanho; i++){
        scanf("%d", &vetor[i]);
    }
    
    int soma = 0;
    for(int i=0; i < tamanho; i++){
        soma+=vetor[i];
    }
    
    printf("A soma dos elementos do vetor Ã©: %d", soma);
    
    return 0;
}
